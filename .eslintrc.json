{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "es6":true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:node/recommended",
        "airbnb",
        "prettier"
    ],

    "plugins": [
        "react",
        "node",
        "prettier"
    ],
    "rules": {
        "no-unused-vars":0,
        "no-console":0,
        "no-process-exit":0
    
    }
}
